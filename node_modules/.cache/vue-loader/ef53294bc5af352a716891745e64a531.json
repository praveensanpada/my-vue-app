n>\n                                    <button\n                                      type=\"button\"\n                                      class=\"btn btn-danger m-2 h-50px\"\n                                    >\n                                      <i\n                                        class=\"\n                                          fab\n                                          fa-ethereum\n                                          text-info\n                                          fab-3x\n                                          mr-5\n                                        \"\n                                      ></i\n                                      >HT ${{ arrayList.ht }}\n                                    </button>\n                                    <button\n                                      type=\"button\"\n                                      class=\"btn btn-secondary m-2 h-50px\"\n                                    >\n                                      <i\n                                        class=\"\n                                          fab\n                                          fa-ethereum\n                                          text-danger\n                                          fab-3x\n                                          mr-5\n                                        \"\n                                      ></i\n                                      >IOTX ${{ arrayList.iotx }}\n                                    </button>\n                                    <button\n                                      type=\"button\"\n                                      class=\"btn btn-warning m-2 h-50px\"\n                                    >\n                                      <i\n                                        class=\"\n                                          fab\n                                          fa-ethereum\n                                          text-primary\n        